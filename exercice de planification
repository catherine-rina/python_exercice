      "metadata": {
        "id": "C3Mhp9wrfiEw"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "cat_pipeline = Pipeline(steps=[\n",
        "    ('imputer', SimpleImputer(strategy='most_frequent')),\n",
        "    ('onehot', OneHotEncoder(handle_unknown='ignore'))])"
      ],
      "metadata": {
        "id": "U-5dio-afk9H"
      },
      "execution_count": 51,
      "outputs": []
