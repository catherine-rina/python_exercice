      "source": [
        "num_pipeline = Pipeline(steps=[\n",
        "    ('imputer', SimpleImputer(strategy='mean')),\n",
        "    ('scaler', StandardScaler())])"
      ],
      "metadata": {
        "id": "iufYvnOBdeiV"
      },
      "execution_count": 50,
      "outputs": []
    },
