        "df_engineered = df_clean.copy()\n",
        "\n",
        "# Taux de remplissage\n",
        "df_engineered['taux_remplissage'] = df_engineered['nb_passagers'] / df_engineered['capacite_bus']\n",
        "\n",
        "# Retard par km et par heure\n",
        "df_engineered['retard_par_km'] = df_engineered['retard_min'] / df_engineered['distance_km']\n",
        "df_engineered['retard_par_heure'] = df_engineered['retard_min'] / df_engineered['duree_prevue_h']\n",
        "\n",
        "# Coût carburant par km\n",
        "df_engineered['cout_carburant_par_km'] = df_engineered['carburant_litre'] / df_engineered['distance_km']\n",
        "\n",
        "# Revenu par passager\n",
        "df_engineered['revenu_par_passager'] = df_engineered['prix_billet'] * df_engineered['nb_passagers']\n",
        "\n",
        "# Ajouter aux features numériques\n",
        "num_features_enhanced = num_features + ['taux_remplissage', 'retard_par_km',\n",
        "                                       'retard_par_heure', 'cout_carburant_par_km',\n",
        "                                       'revenu_par_passager']\n",
        "\n",
        "X_enhanced = df_engineered[num_features_enhanced + cat_features]"
      ],
